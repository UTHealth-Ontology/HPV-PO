        <obo:IAO_0000233 rdf:resource="https://github.com/information-artifact-ontology/IAO/issues/250"/>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2022-01-28T07:20:08Z</dc:date>
